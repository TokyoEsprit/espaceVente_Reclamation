<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="Gui.AfficherProduitAdminController">
   <children>
      <AnchorPane id="AnchorPane" fx:id="chercher" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="table" layoutX="14.0" layoutY="28.0" prefHeight="230.0" prefWidth="509.0">
               <columns>
                  <TableColumn fx:id="nom" prefWidth="75.0" text="Nom" />
                  <TableColumn fx:id="descrip" prefWidth="75.0" text="Description" />
                  <TableColumn fx:id="prix" prefWidth="75.0" text="Prix" />
                  <TableColumn fx:id="etat" prefWidth="75.0" text="Etat" />
                  <TableColumn fx:id="img" prefWidth="75.0" text="Image" />
                  <TableColumn fx:id="local" prefWidth="75.0" text="localisation" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor>
            </TableView>
            <Button fx:id="supp" layoutX="26.0" layoutY="277.0" mnemonicParsing="false" onAction="#supprimer" text="supprimer" />
            <Button fx:id="Chercher" layoutX="321.0" layoutY="277.0" mnemonicParsing="false" onAction="#Chercher" text="Chercher" />
            <TextField fx:id="ch" layoutX="416.0" layoutY="277.0" />
            <Button fx:id="Reclamation" layoutX="26.0" layoutY="321.0" mnemonicParsing="false" onAction="#Reclamation" prefHeight="25.0" prefWidth="149.0" text="Consulter Reclamation" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
